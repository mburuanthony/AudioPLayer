{
  "expo": {
    "name": "AudioPlayer",
    "slug": "AudioPlayer",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/img/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#0F0E0E"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "infoPlist": {
        "UIBackgroundModes": [
          "audio"
        ]
      },
      "supportsTablet": false,
      "bundleIdentifier": "com.mburuantony.AudioPlayer"
    },
    "android": {
      "userInterfaceStyle": "automatic",
      "package": "com.mburuantony.AudioPlayer"
    },
    "extra": {
      "eas": {
        "projectId": "2c983823-dccf-499b-98c4-5c737fe551ab"
      }
    }
  }
}
